// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model ksiazki {
    id Int @id @default(autoincrement())
    tytul String
    autor String
    data_wydania DateTime
}

model wypozyczalnia {
    id Int @id @default(autoincrement())
    id_k Int
    id_c Int
    data_od DateTime
    data_do DateTime
}

model czytelnicy {
    id Int @id @default(autoincrement())
    imie String
    nazwisko String
    email String
    telefon String 
}